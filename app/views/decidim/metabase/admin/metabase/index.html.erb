<div class="card" id="metabase">
  <div class="card-divider">
    <h2 class="card-title">
      <%= t "decidim.admin.metabase.title" %>
    </h2>
  </div>
  <div class="card-section">
    <% if urls.blank? %>
      <%= t("decidim.admin.metabase.not_found") %>
    <% else %>
      <% urls.each do |url| %>
        <iframe src="<%= url %>"
                width="100%"
                height="4200px">
        </iframe>
      <% end %>
    <% end %>
  </div>
</div>
